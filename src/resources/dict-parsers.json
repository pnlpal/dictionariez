{
  "google": {
    "url": "https://www.google.com/search?q=define+<word>",
    "languages": ["German", "Spanish", "French", "Italian"],
    "credentials": "same-origin",
    "result": {
      "w": {
        "selector": "[data-dobid='hdw']"
      },
      "langSymbol": {
        "selector": "[data-language]",
        "attr": "data-language"
      },
      "prons": [
        {
          "symbol": {
            "selector": ".vmod[data-topic][data-hveid]",
            "htmlRegex": "<span>([^<>]*)</span>",
            "regexIndex": 1
          },
          "type": "unknow"
        }
      ],
      "defs": {
        "groups": ".vmod>.vmod[data-topic]",
        "result": {
          "pos": {
            "selector": "i"
          },
          "def": {
            "selector": "[data-dobid='dfn']",
            "toArray": true,
            "includeArrayIndex": true
          }
        }
      },

      "defs2": {
        "groups": "[data-tae]>[data-mh='-1']",
        "result": {
          "pos": {
            "selector": "div"
          },
          "def": {
            "selector": "ol>li",
            "max": 2,
            "toArray": true
          }
        }
      },
      "images": {
        "groups": ".vmod [data-topic] g-img",
        "result": {
          "src": {
            "selector": "img",
            "attr": "title"
          }
        }
      }
    }
  },
  "bingCN": {
    "url": "https://cn.bing.com/dict/search?mkt=zh-cn&q=<word>",
    "supportChinese": true,
    "result": {
      "w": {
        "selector": "#headword"
      },
      "prons": [
        {
          "symbol": {
            "selector": ".hd_area .hd_prUS"
          },
          "audio": {
            "selector": ".hd_area .hd_prUS + .hd_tf a",
            "attr": "data-mp3link",
            "srcOrigin": "https://cn.bing.com"
          },
          "type": "ame"
        },
        {
          "symbol": {
            "selector": ".hd_area .hd_pr"
          },
          "audio": {
            "selector": ".hd_area .hd_pr + .hd_tf a",
            "attr": "data-mp3link",
            "srcOrigin": "https://cn.bing.com"
          },
          "type": "bre"
        },
        {
          "symbol": {
            "selector": ".hd_area .hd_p1_1"
          },
          "type": "pinyin"
        }
      ],
      "defs": {
        "groups": "#homoid tr.def_row",
        "result": {
          "pos": {
            "selector": ".pos"
          },
          "def": {
            "selector": ".def_fl>.de_li1",
            "toArray": true,
            "strFilter": "\\d+[.]"
          }
        }
      },
      "defs2": {
        "groups": ".qdef ul>li",
        "result": {
          "pos": {
            "selector": ".pos"
          },
          "def": {
            "selector": ".def"
          }
        }
      }
    }
  },
  "wiktionary": {
    "url": "https://en.wiktionary.org/wiki/<word>",
    "languages": [
      "German",
      "Spanish",
      "French",
      "Italian",
      "Portuguese",
      "Swedish",
      "Finnish",
      "Norwegian",
      "Danish",
      "Polish",
