  "Tajik": {
    "url": "https://vazhaju.tj/search/<word>",
    "languages": ["Tajik"],
    "result": {
      "w": {
        "selector": ".word .word-spelling>span"
      },
      "prons": [
        {
          "symbol": {
            "selector": ".word .word-spelling [dir='rtl']"
          }
        }
